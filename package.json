{
  "name": "ao-core",
  "version": "1.0.15",
  "description": "AO - Core",
  "main": "dist/index.js",
  "bin": {
    "ao-core": "bin/bin.js"
  },
  "scripts": {
    "start": "cross-env DEBUG=ao:* node dist/bin.js --networkId 4",
    "start:mainnet": "cross-env DEBUG=ao:* node dist/bin.js --networkId 1",
    "build": "npm run clean && cross-env NODE_ENV=development webpack",
    "build-prod": "npm run clean && cross-env NODE_ENV=production webpack --progress",
    "clean": "rimraf dist",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "test-module": "mocha -r ts-node/register src/modules/${MODULE:-**}/*.spec.ts",
    "test-module-eth": "MODULE=eth npm run test-module",
    "test-module-dat": "MODULE=dat npm run test-module",
    "test-ao-crypto": "mocha -r ts-node/register src/AOCrypto.spec.ts",
    "list-native-deps": "node ./node_modules/node-has-native-dependencies/index.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "ao-contracts": "git+ssh://git@git.y-designs.com:ao/ao-contracts.git#v1.0.8",
    "apollo-server-express": "^1.3.6",
    "apollo-upload-server": "^5.0.0",
    "archiver": "^3.0.0",
    "assert": "^1.4.1",
    "bignumber.js": "^7.2.1",
    "body-parser": "^1.18.3",
    "casual": "^1.5.19",
    "checksum": "^0.1.1",
    "cors": "^2.8.4",
    "dat-node": "^3.5.11",
    "debug": "^3.1.0",
    "discovery-swarm": "^5.1.2",
    "eccrypto": "git://github.com/ryuheiyokokawa/eccrypto.git",
    "eth-crypto": "1.2.3",
    "express": "^4.16.3",
    "ffprobe": "^1.1.0",
    "ffprobe-static": "^2.0.0",
    "fs-extra": "^6.0.1",
    "fuse.js": "3.2.1",
    "got": "git://github.com/ryuheiyokokawa/got.git#ao",
    "graphql": "^0.13.2",
    "graphql-import-loader": "^0.2.1",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^3.0.2",
    "hyperdb": "^3.5.0",
