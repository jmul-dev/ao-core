{
    "name": "ao-core",
    "version": "1.0.19",
    "description": "AO - Core",
    "main": "dist/index.js",
    "bin": {
        "ao-core": "bin/bin.js"
    },
    "scripts": {
        "compile": "npm run compile-patch && tsc && node scripts/copy-non-ts-files.js",
        "compile-patch": "rimraf node_modules/@types/bn.js",
        "start": "cross-env NODE_ENV=development DEBUG=ao:*,dat-node node dist/bin.js --ethNetworkRpc wss://rinkeby.infura.io/ws",
        "start:mainnet": "cross-env DEBUG=ao:* node dist/bin.js --ethNetworkRpc wss://mainnet.infura.io/ws",
        "build": "npm run clean && npm run compile && npm run copy-dependencies",
        "clean": "rimraf dist",
        "test": "mocha --exit -r ts-node/register src/**/*.spec.ts",
        "test-module": "mocha --exit -r ts-node/register src/modules/${MODULE:-**}/*.spec.ts",
        "test-module-eth": "cross-env MODULE=eth npm run test-module",
        "test-module-dat": "cross-env MODULE=dat DEBUG=dat-node npm run test-module",
        "test-module-fs": "cross-env MODULE=fs npm run test-module",
        "test-module-p2p": "cross-env MODULE=p2p npm run test-module",
        "test-ao-crypto": "mocha --exit -r ts-node/register src/AOCrypto.spec.ts",
        "test-aodb": "mocha --exit -r ts-node/register src/modules/p2p/AODB.spec.ts",
        "test-taodb": "mocha --exit -r ts-node/register src/modules/p2p/TaoDB.spec.ts",
        "test-p2p": "mocha --exit -r ts-node/register src/modules/p2p/p2p.spec.ts",
        "list-native-deps": "node ./node_modules/node-has-native-dependencies/index.js",
        "debug": "node --inspect --nolazy dist/bin.js",
        "copy-dependencies": "copy-node-modules . dist --concurrency=16"
    },
    "files": [
        "dist",
        "src"
    ],
    "dependencies": {
        "ao-contracts": "git+ssh://git@git.y-designs.com:ao/ao-contracts.git#v1.0.11",
        "ao-library": "git+ssh://git@git.y-designs.com:ao/ao-library.git",
        "aodb": "^1.1.4",
        "apollo-server-express": "^1.3.6",
        "archiver": "^3.0.0",
        "assert": "^1.4.1",
        "bignumber.js": "^7.2.1",
        "body-parser": "^1.18.3",
        "casual": "^1.5.19",
        "checksum": "^0.1.1",
        "cors": "^2.8.4",
        "dat-node": "^3.5.11",
        "debug": "^3.1.0",
        "discovery-swarm": "^5.1.2",
        "eth-crypto": "1.2.3",
        "express": "^4.16.3",
        "ffprobe": "^1.1.0",
        "ffprobe-static": "^2.0.0",
        "fs-extra": "^6.0.1",
        "fuse.js": "3.2.1",
        "graphql": "^0.13.2",
        "graphql-import-node": "0.0.2",
        "graphql-tag": "^2.10.1",
        "graphql-tools": "^3.0.2",
        "graphql-upload": "^8.0.4",
        "md5": "^2.2.1",
        "minimist": "^1.2.0",
        "nedb": "^1.8.0",
        "queue": "^4.5.0",
        "unzipper": "^0.9.3",
        "web3": "^1.0.0-beta.33",
        "web3-legacy": "git://github.com/nharlow89/web3.js.git#legacy",
        "web3-utils": "^1.0.0-beta.50",
        "winston": "^3.1.0",
        "yargs": "^12.0.2"
    },
    "devDependencies": {
        "@types/chai": "^4.1.4",
        "@types/debug": "0.0.30",
        "@types/express": "^4.16.0",
        "@types/graphql": "^0.13.1",
        "@types/minimist": "^1.2.0",
        "@types/mocha": "^5.2.5",
        "@types/nedb": "^1.8.5",
        "@types/node": "^10.3.1",
        "@types/web3": "^1.0.18",
        "chai": "^4.1.2",
        "copy-node-modules": "^1.1.1",
        "copyfiles": "^2.1.0",
        "cross-env": "^5.2.0",
        "mocha": "^5.2.0",
        "node-has-native-dependencies": "^1.0.2",
        "random-access-memory": "^3.1.1",
        "recursive-copy": "^2.0.10",
        "rimraf": "^2.6.2",
        "ts-node": "^7.0.0",
        "tsc-watch": "^1.0.31",
        "tslint": "^5.10.0",
        "typescript": "3.1.1"
    }
}
